<local:BaseWindow x:Class="CacxClient.Windows.CreateAccWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:CacxClient.Windows"
        xmlns:cc="clr-namespace:CacxClient.CostumComponents"
        mc:Ignorable="d"
        Title="CreateAccWindow" Style="{StaticResource SmallWindowStyle}">
    <Grid>
        <cc:TextBoxTextBlock Label="Email:" Width="200" Height="35" Margin="0, 0, 0, 100"/>
        <cc:TextBoxTextBlock Label="Password:" Width="200" Height="35" Margin="0, 50, 0, 0"/>
        <Button x:Name="GeneratePwBtn" Style="{StaticResource IconBtnStyle}" Width="35" Height="35" Content="🎲" Margin="250, 50, 0, 0"/>
    </Grid>
</local:BaseWindow>
